#import( "/_cascade/formats/helpers.velocity" )
#import( "/_cascade/formats/helpers/debug.velocity" )

#set ( $simpleArray = ['a', 'b', 'c'] )
#set ( $nestedArray = [
  ['a', 'A'],
  ['b', 'B']
] )
#set ( $simpleMap = {'a':1, 'b':2} )

<div id="debug-format">
    <h2>Debug Format</h2>

    <pre>simpleArray: $simpleArray</pre>
    <pre>nestedArray: $nestedArray</pre>
    <pre>simpleMap: $simpleMap</pre>
    <pre>simpleMap['a']: $simpleMap['a']</pre>
    <pre>simpleMap['nope']: $simpleMap['nope']</pre>

    <h4>contentRoot</h4>
    <pre>#inspectXML( $contentRoot )</pre>
</div>
